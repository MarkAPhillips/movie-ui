{
  "name": "movie-ui",
  "version": "1.0.18",
  "description": "Movie UI built in React",
  "main": "index.js",
  "scripts": {
    "pretest": "npm run lint",
    "version": "echo $npm_package_version",
    "test": "jest --config ./jest.config.json",
    "test:watch": "npm test -- --watch",
    "test:ci": "npm test -- --ci --runInBand --reporters=default --reporters=jest-junit",
    "build:env": "chmod a+x ./build/env.sh && ./build/env.sh",
    "build": "webpack -p --env=prod",
    "start": "npm run build:env && webpack-dashboard -- webpack-dev-server --open",
    "start:docker": "npm run docker:build && npm run docker:run",
    "lint": "eslint --quiet \"src/**\"",
    "docker:build": "docker build -f ./build/local/Dockerfile -t movie-ui .",
    "docker:run": "docker rm -f movie-ui &> /dev/null || true && docker run --name movie-ui -e PORT=8080 -p 8080:8080 -it movie-ui"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MarkAPhillips/movie-ui.git"
  },
  "keywords": [],
  "author": "Mark Phillips",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MarkAPhillips/movie-ui/issues"
  },
  "homepage": "https://github.com/MarkAPhillips/movie-ui#readme",
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@types/lodash": "^4.14.151",
    "@types/react-router-dom": "^5.1.5",
    "apollo-boost": "^0.4.7",
    "graphql": "^15.0.0",
    "lodash": "^4.17.15",
    "moment": "^2.25.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "@apollo/react-testing": "^3.1.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@testing-library/dom": "^7.2.2",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.4",
    "@types/jest": "^25.2.1",
    "@types/react": "^16.9.26",
    "@types/react-dom": "^16.9.5",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "babel-loader": "^8.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.0.3",
    "html-webpack-skip-assets-plugin": "0.0.2",
    "jest": "^25.5.3",
    "jest-junit": "^10.0.0",
    "ts-jest": "^25.4.0",
    "ts-lint": "^4.5.1",
    "ts-loader": "^7.0.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dashboard": "^3.2.0",
    "webpack-dev-server": "^3.10.3"
  }
}
